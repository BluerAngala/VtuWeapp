page{
  background: var(--mainColor);
}

.Top{
  width: 100%;
  background: #fff;
  display: inline-block;
  border-bottom: 2px dashed var(--mainColor);
  position: relative;
  padding-bottom: 70px;
}
.Logo{
  width: 60px;
  height: 60px;
  margin: 20px auto;
  background: #fff;
  border-radius: 100px;
  border: 2px solid #fff;
  box-shadow: 0px 0px 10px #b9b9b9;
}
.Logo image{
  width: 100%;
  height: 100%;
  border-radius: 100px;
}

.Title{
  width: 100%;
  text-align: center;
  margin-top: 10px;
}
.Title_H1{
  width: 100%;
  height: 30px;
  line-height: 30px;
  font-size: 22px;
  font-weight: bold;
  margin-bottom: 10px;
  color: #000;
}
.Title_H2{
  width: 100%;
  height: 20px;
  line-height: 20px;
  font-size: 14px;
  color: var(--h1Color);
}

.ShareBtn{
  width: 80px;
  height: 40px;
  line-height: 45px;
  text-align: center;
  background: var(--mainColor);
  color: #fff;
  font-size: 14px;
  font-weight: bold;
  border-radius: 100px 100px 0 0 ;
  box-shadow: 0 -2px 2px #adadad;
  position: absolute;
  bottom: -2px;
  left: 50%;
  transform: translate(-50%, 0);
}
.List{}
.List_Title{
  width: 100%;
  height: 40px;
  line-height: 40px;
  text-align: center;
  font-size: 12px;
  color: #fff;
  margin: 10px 0;
  position: relative;
}
.List_Title .label{
  width: 100px;
  height: 100%;
  margin: 0px auto;
  background: var(--mainColor);
  position: relative;
  z-index: 1;
}
.List_Title .line{
  width: 180px;
  height: 1px;
  background: #eee;
  margin: -18px auto;
  position: relative;
  z-index: 0;
}
.List_Li{
  padding: 10px;
  height: 40px;
  width: calc(100% - 20px);
  margin-bottom: 10px;
}
.List_Li .image{
  width: 40px;
  height: 40px;
  float: left;
  background: #fff;
  border-radius: 100px;
}
.List_Li .image image{
  width: 30px;
  height: 30px;
  margin: 5px;
  border-radius: 100px;
}
.List_Li .detail{
  width: calc(100% - 80px);
  float: left;
  height: 100%;
  margin-left: 20px;
}
.List_Li .detail .detal_top{
  width: 100%;
  height: 25px;
  line-height: 25px;
  color: #fff;
}
.List_Li .detail .detal_top .name{
  width: auto;
  float: left;
  font-size: 15px;
}
.List_Li .detail .detal_top .icon{
  width: auto;
  float: left;
  height: 100%;
  margin-left: 10px;
  font-size: 14px;
  color: var(--secondColor);
}
.List_Li .detail .detai_bottom{
  width: 100%;
  height: 15px;
  line-height: 15px;
  font-size: 11px;
  color: var(--h4Color);
}
.List_Li .right{
  width: 20px;
  height: 40px;
  line-height: 40px;
  float: right;
  font-size: 12px;
  color: var(--h2Color);
  text-align: center;
}
.About_Empty{
  width: 100%;
  height: 50px;
  margin: 100px auto;
  text-align: center;
}
.About_Empty image{
  width: 50px;
  height: 50px;
}

.Kefu{
  width: 100%;
  height: 30px;
  margin-top: 20px;
  margin-bottom: 20px;
}
.Kefu button{
  background: #fff !important;
  color: var(--mainColor);
  font-size: 12px;
  width: 200px !important;
}
