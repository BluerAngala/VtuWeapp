}
.LuckDraw_Style_Shrink{
    height: 40px;
    line-height: 40px;
    font-size: 14px;
}
.LuckDraw_Style_Shrink_Name{
    width: calc(100% - 90px);
    float: left;
    color: var(--lessColor);
}
.LuckDraw_Style_Shrink_Time{
    width: 70px;
    height: 100%;
    float: left;
    color: var(--h1Color);
    font-size: 12px;
}
.LuckDraw_Style_Shrink_Right{
    float: right;
    width: 20px;
    text-align: center;
    color: var(--h1Color);
    font-size: 12px;
}
.LuckDraw_Style_Image{
    width: 100%;
    height: auto;
    float: left;
    position: relative;
}
.LuckDraw_Style_Image .image{
    width: 100%;
    height: 0;
    border-radius: var(--radius) var(--radius) 0 0;
}
.LuckDraw_Style_Image.Empty{
    width: 100%;
    height: 25px;
    line-height: 25px;
    text-align: center;
    font-size: 12px;
    padding: 10px 0;
    border-radius: var(--radius) var(--radius) 0 0;
}
.LuckDraw_Style_Image .createName{
    position: absolute;
    top: 10px;
    right: 10px;
    background: rgba(104, 104, 104, 0.5);
    color: #fff;
    font-size: 10px;
    padding: 2px 10px;
}

.LuckDraw_Style_Info,
.Vote_Style_Info{
    float: left;
    padding-top: 10px;
    padding-bottom: 10px;
    position: relative;
}
.Vote_Style_Info_Name{
    width: 100%;
    font-size: 14px;
    font-weight: bold;
    margin-top: 10px;
}

.Creater{
    border-radius:var(--radius);
    height: 30px;
    line-height: 25px;
    width: 100%;
    color: var(--redColor);
    font-size: 14px;
    font-weight: bold;
}
.Items{
    width: 100%;
    float: left;
}
.Items_Title{
    width: 40px;
    float: left;
    font-size: 14px;
}
.Items_List{
    width: calc(100% - 40px);
    float: right;
}
.Items_List_Li{
    width: 100%;
    height: 25px;
    color: var(--lessColor);
    font-size: 12px;
}
.Items_List_Li_Name{
    float: left;
    width: calc(100% - 90px);
    font-size: 14px;
}
.Items_List_Li_Price{
    width: 80px;
    height: 100%;
    float: right;
    text-align: right;
}
.Detail{
    width: 100%;
    height: 20px;
    float: left;
    margin-top: 10px;
    line-height: 20px;
    font-size: 11px;
}
.Detail_Li{
    float: left;
    margin-right: 5px;
    font-size: 11px;
}
.Detail_Li.Active{
    color: var(--mainColor);
}
.Detail_Li.Time{
    color: var(--h1Color);
}
.Detail_Li.Icon{
    height: 14px;
    background: var(--mainColor);
    color: #fff;
    margin: 3px 10px 3px 0px;
    line-height: 14px;
    font-size: 11px;
    padding: 0px 2px;
    border-radius: 2px;
}
.Detail_Li.Second{
    background: var(--secondColor);
}
.Detail_Li.Pass{
    background: #fff;
    color: var(--h1Color);
    border: 1px solid var(--h1Color);
    height: 12px;
    line-height: 12px;
}
.Detail_Li.Phone{
    background: #1b8ed2;
}
.Detail_Li_Icon{
    width: 14px;
    height: 14px;
    text-align: center;
    float: left;
    margin: 3px 3px 3px 0px;
    line-height: 14px;
    font-size: 12px;
}
.Detail_Li_Text{
    float: left;
}

.Users{
    width: 100%;
    height: 20px;
    float: left;
    margin-top: 5px;
    line-height: 20px;
    position: relative;
}
.Users_List{
    width: auto;
    float: right;
}
.Users_List_Li{
    width: 20px;
    height: 20px;
    float: left;
    background: #eee;
    margin-left: -5px;
    text-align: center;
    color: #fff;
    font-size: 12px;
}
.Users_List_Li:first-child{
    margin-left: 0px;
}
.Users_Count{
    float: left;
    font-size: 11px;
    color: var(--h1Color);
    position: relative;
}
.Users_Count span{
    height: 20px;
    background: var(--secondColor);
    color: #fff;
    border-radius: 4px;
    padding: 0px 5px;
    position: relative;
    margin-left: 15px;
}
.Users_Count span:before{
    content: '';
    width: 0;
    height: 0;
    position: absolute;
    top: 50%;
    left: -5px;
    transform: translate(0, -50%);
    border-top: 5px solid transparent;
    border-bottom: 5px solid transparent;
    border-right: 5px solid var(--secondColor);
}
.Winner{
    width: 40px;
    height: 40px;
    position: absolute;
    right: 30px;
    bottom: 25px;
    color: var(--secondColor);
    font-size: 60px;
}
.Winner.fail{
    color: var(--h2Color);
}
.OverTime_Style{
    padding-top: var(--pagePadding);
    padding-bottom: var(--pagePadding);
    position: relative;
    line-height: 30px;
    font-size: 14px;
}
.OverTime_Style_Center{
    width: calc(100% - 90px);
    float: left;
}
.LuckDraw_OverTime_Style_Center_Title{
    width: 40px;
    float: left;
}
.LuckDraw_OverTime_Style_Center_Product{
    width: calc(100% - 40px);
    float: left;
}
.Vote_OverTime_Style_Center_Name{
    font-size: 14px;
    color: var(--lessColor);
    font-weight: bold;
}
.OverTime_Style_Time{
    width: 70px;
    font-size: 12px;
    color: var(--h1Color);
    text-align: right;
    float: left;
}
.OverTime_Style_Right{
    float: right;
    width: 20px;
    font-size: 12px;
    color: var(--h4Color);
    text-align: right;
}
/*-------------------------投票部分-------------------------------*/
