/**/
.Bottom_Style{
    width: 100%;
    height: 90px;
    background: #fff;
    position: fixed;
    left: 0px;
    bottom: 0px;
    box-shadow: 0px -1px 2px #eee;
    display: flex;
    flex: 1;
    z-index: 10;
}
.Bottom_Style.animation{
    bottom: -150px;
}
.Bottom_Style_Li{
    width: 40%;
    height: 100%;
    text-align: center;
    color: var(--h1Color);
    position: relative;
}
.Bottom_Style_Li:first-child,
.Bottom_Style_Li:last-child{
    width: 30%;
}
.Bottom_Style_Li_Icon{
    width: 30px;
    height: 30px;
    margin: 20px auto 5px auto;
    text-align: center;
    line-height: 30px;
    font-size: 30px;
}
.Bottom_Style_Li_Text{
    width: 100%;
    height: 20px;
    line-height: 20px;
    font-size: 12px;
}
.Bottom_Style_Li_Center{
    width: 80px;
    height: 80px;
    margin: 5px auto;
    text-align: center;
}
.Bottom_Style_Li_Center.formView{
    position: fixed;
    left:50%;
    transform: translate(-50%, 0);
    bottom: -150px;
}
.Bottom_Style_Li_Center_Add{
    width: 72px;
    height: 72px;
    border: 4px solid var(--page);
    background: -webkit-linear-gradient(left, #ff906e 0%, var(--secondColor) 100%);
    font-size: 14px;
    color: #fff;
    box-shadow: 0px 0px 2px #a1a1a1;
    position: relative;
}
.Bottom_Style_Li_Center_Add.common:after{
    content: '';
    width: 100%;
    height: 100%;
    position: absolute;
    animation: BottomCenterAnimation 2s ease infinite;
    background: var(--secondColor);
    border-radius: 100px;
    left: 0px;
    top: 0px;
    transition: all 1s;
    z-index: -1;
}
.Bottom_Style_Li_Center_Add.fail{
    background: var(--h2Color);
    color: #fff;
}
.Bottom_Style_Li_Center_Add text{
    line-height: 20px;
    position: absolute;
    top: 50%;
    left: 0px;
    width: 100%;
    transform: translate(0, -50%);
    text-align: center;
}
.Bottom_Style_Li_Info{
    position: absolute;
    top: -40px;
    height: 30px;
    line-height: 30px;
    padding: 0px 10px;
    font-size: 12px;
    background: var(--mainColor);
    left: 50%;
    transform: translate(-50%, 0);
    animation: BottomInfoAnimation 3s ease infinite;
    white-space : nowrap;
}
.Bottom_Style_Li_Info:after{
    position: absolute;
    content: '';
    width: 0px;
    height: 0px;
    border-left: 8px solid transparent;
    border-top: 4px solid var(--mainColor);
    bottom: -4px;
    left: 50%;
    transform: translate(-50%, 0);
}
@keyframes BottomInfoAnimation{
    0% {
        opacity: 0.6;
        top: -40px;
    }
    50% {
        top: -30px;
        opacity: 1;
    }
    100% {
        opacity: 0.6;
        top: -40px;
    }
}
@keyframes BottomCenterAnimation{
    0% {
        transform: scale(1);
        opacity: 0.5;
    }
    100% {
        transform: scale(1.4);
        opacity: 0.0;
    }
}

