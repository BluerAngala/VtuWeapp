}
.Vote_Users_Info_Style_Prop_Btn_Li{
    width: 50%;
    height: 100%;
    float: left;
    background: var(--mainColor);
    color: #fff;
    font-size: 12px;
    text-align: center;
    line-height: 40px;
    border-radius: var(--radius) 0px var(--radius) 0;
}
.Vote_Users_Info_Style_Prop_Btn_Li.help{
    color: var(--mainColor);
    background: transparent;
    border-radius: 0 var(--radius)  0 var(--radius);
}

